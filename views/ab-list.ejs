<%- include('parts/html-head') %>
<%- include('parts/navbar') %>

<div class="container">
    <div class="row">
        <div class="col">

            <table class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">品種</th>
                    <th scope="col">類別</th>
                    <th scope="col">性別</th>
                    <th scope="col">來源</th>
                    <th scope="col">我在</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(let i of rs){ %>
                  <tr>
                    <td><%= i.sid %></td>
                    <td><%= i.variety %></td>
                    <td><%= i.class %></td>
                    <td><%= i.sex %></td>
                    <td><%= i.origin %></td>
                    <td><%= i.place %></td>
  
                  </tr>
                  <% } %>
                </tbody>
            </table>
        </div>
    </div>



</div>
